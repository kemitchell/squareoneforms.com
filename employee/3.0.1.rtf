\par \pard\plain \s0\sl240\slmult1\qj\widctlpar\sb120\sa120\ltrpar\hyphpar0\kerning0\cf0\rtlch\af8\afs22\alang1025\ltrch\hich\af5\afs24\alang1033\dbch\af9\langfe1033\loch\f5\fs24\lang1033\ql{\ulc0\ulnone\b
